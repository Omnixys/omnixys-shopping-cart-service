    "typeorm": "0.3.21"
  },
  "devDependencies": {
    "@tsconfig/node23": "23.0.0",
    "@types/compression": "1.7.5",
    "@types/express": "^4.17.17",
    "@types/graphql": "14.2.3",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "4.0.9",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
