    "@nestjs/core": "11.0.12",
    "@nestjs/graphql": "13.0.3",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/typeorm": "11.0.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.58.0",
