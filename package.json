  "overrides": {
    "nest-keycloak-connect": {
      "@nestjs/common": "$@nestjs/common",
      "@nestjs/core": "$@nestjs/core",
      "rxjs": "7.8.2"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
